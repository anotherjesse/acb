{
  "name": "rc",
  "version": "1.0.0",
  "description": "Matrix MetaOrchestrator + AgentBridge for Codex",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/meta-orchestrator/index.ts",
    "dev:orchestrator": "tsx src/meta-orchestrator/index.ts",
    "dev:bridge": "tsx src/agent-bridge/index.ts",
    "init-dev": "tsx src/meta-orchestrator/init-dev.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/meta-orchestrator/index.js",
    "start:orchestrator": "node dist/meta-orchestrator/index.js",
    "start:bridge": "node dist/agent-bridge/index.js",
    "test": "tsx --test $(find src -name '*.test.ts' -print)"
  },
  "keywords": [
    "codex",
    "matrix",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openai/codex-sdk": "^0.98.0",
    "dotenv": "^17.2.3",
    "telegraf": "^4.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
